---
import { CommandList as CommandListReact } from '@/components/ui/command';

interface Props {
  class?: string;
  className?: string;
}

const { class: classAstro, className, ...props } = Astro.props;
---

<CommandListReact client:load class={classAstro} className={className} {...props}>
  <slot />
</CommandListReact> 