---
import { CommandItem as CommandItemReact } from '@/components/ui/command';

interface Props {
  class?: string;
  className?: string;
}

const { class: classAstro, className, ...props } = Astro.props;
---

<CommandItemReact client:load class={classAstro} className={className} {...props}>
  <slot />
</CommandItemReact> 