---
import { CommandShortcut as CommandShortcutReact } from '@/components/ui/command';

interface Props {
  class?: string;
  className?: string;
}

const { class: classAstro, className, ...props } = Astro.props;
---

<CommandShortcutReact client:load class={classAstro} className={className} {...props}>
  <slot />
</CommandShortcutReact> 