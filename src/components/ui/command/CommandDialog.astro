---
import { CommandDialog as CommandDialogReact } from '@/components/ui/command';

interface Props {
  class?: string;
  className?: string;
}

const { class: classAstro, className, ...props } = Astro.props;
---

<CommandDialogReact client:load class={classAstro} className={className} {...props}>
  <slot />
</CommandDialogReact> 