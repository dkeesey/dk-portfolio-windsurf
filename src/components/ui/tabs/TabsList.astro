---
import { TabsList as TabsListReact } from '@/components/ui/tabs';

interface Props {
  class?: string;
  className?: string;
}

const { class: classAstro, className, ...props } = Astro.props;
const combinedClassName = [classAstro, className].filter(Boolean).join(' ');
---

<TabsListReact client:load className={combinedClassName} {...props}>
  <slot />
</TabsListReact>
