---
import { CardTitle as CardTitleReact } from '@/components/ui/card';

interface Props {
  class?: string;
  className?: string;
}

const { class: classAstro, className, ...props } = Astro.props;
---

<CardTitleReact client:load class={classAstro} className={className} {...props}>
  <slot />
</CardTitleReact> 