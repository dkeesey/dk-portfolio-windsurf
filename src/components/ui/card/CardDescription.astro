---
import { CardDescription as CardDescriptionReact } from '@/components/ui/card';

interface Props {
  class?: string;
  className?: string;
}

const { class: classAstro, className, ...props } = Astro.props;
---

<CardDescriptionReact client:load class={classAstro} className={className} {...props}>
  <slot />
</CardDescriptionReact> 