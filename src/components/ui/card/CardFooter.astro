---
import { CardFooter as CardFooterReact } from '@/components/ui/card';

interface Props {
  class?: string;
  className?: string;
}

const { class: classAstro, className, ...props } = Astro.props;
---

<CardFooterReact client:load class={classAstro} className={className} {...props}>
  <slot />
</CardFooterReact> 