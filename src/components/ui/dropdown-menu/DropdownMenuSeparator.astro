---
import { DropdownMenuSeparator as DropdownMenuSeparatorReact } from '@/components/ui/dropdown-menu';

interface Props {
  class?: string;
  className?: string;
}

const { class: classAstro, className, ...props } = Astro.props;
---

<DropdownMenuSeparatorReact 
  client:load 
  class={classAstro} 
  className={className}
  {...props}
>
  <slot />
</DropdownMenuSeparatorReact> 