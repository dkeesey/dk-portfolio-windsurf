---
import { DialogFooter as DialogFooterReact } from '@/components/ui/dialog';

interface Props {
  class?: string;
  className?: string;
}

const { class: classAstro, className, ...props } = Astro.props;
const combinedClassName = [classAstro, className].filter(Boolean).join(' ');
---

<DialogFooterReact client:load className={combinedClassName} {...props}>
  <slot />
</DialogFooterReact>
