---
import { DialogTitle as DialogTitleReact } from '@/components/ui/dialog';

interface Props {
  class?: string;
  className?: string;
}

const { class: classAstro, className, ...props } = Astro.props;
const combinedClassName = [classAstro, className].filter(Boolean).join(' ');
---

<DialogTitleReact client:load className={combinedClassName} {...props}>
  <slot />
</DialogTitleReact>
